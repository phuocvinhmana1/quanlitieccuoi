@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model  IEnumerable<Modelss.Model.DichVu>


@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Danh sách dịch vụ</h2>

<p>
    @Html.ActionLink("Thêm mới", "themDichVu")
</p>
<table class ="table">
    <tr>
        <th>
            Tên dịch vụ
        </th>
        <th>
            Loại dịch vụ
        </th>
        <th>
            Giá tiền
        </th>
        <th>
            Trạng thái
        </th>
        <th>
            Ghi chú
        </th>
        <th>
            Mô tả
        </th>
        <th>
            Hình ảnh
        </th>
        
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenDichVu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoaiDichVu1.TenLoaiDichVu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GiaTien)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrangThai)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GhiChu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MoTa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Anh)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MaDichVu }) |
        
            @Html.ActionLink("Xóa", "Delete", new { id=item.MaDichVu })
        </td>
    </tr>
}

</table>

