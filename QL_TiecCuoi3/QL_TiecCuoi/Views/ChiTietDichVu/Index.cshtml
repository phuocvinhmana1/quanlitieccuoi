@{

    Layout = "~/Views/Shared/_Layout.cshtml";
    
}
@{
    var session = (Modelss.Common.UserLogin)Session[Modelss.Common.CommonConstans.TENDANGNHAP_SESSTION];
    }

@{if(session != null)
      {
          <ul style="margin-left:1000px"> 
              <li>Xin chào,   @session.tendangnhap</li>
          </ul>
       
        <ul style="margin-left:1000px">
            <li>@Html.ActionLink("Thoát","DangXuat","DangNhap")</li>
        </ul>
    }
    }

@using Modelss.Model;

@model DichVu

<div class="container minheight" >
    <hr style="width:100%"/>
    <div class="row">
        <div class="col-md-6">
            <img src="../Content/hinhanh/anhdichvu/@Model.Anh" width="500" height="300"/>
        </div>
        <div class="col-md-6">
            <p>Mã món ăn: @Model.MaDichVu</p>
            <h3>Tên sản phẩm: @Model.TenDichVu</h3>
            <p>Mô tả: @Model.MoTa</p>

 
           @*   @{ Html.RenderAction("layTenMonAn", "ThucDon", new { thucdon = @Model.MaThucDon }); }*@
            <h4>Giá tham khảo: @Model.GiaTien VND</h4>
            
      
             <a href="@Url.Action("themdv", "HoSoKH", new {madichvu = @Model.MaDichVu })" class="genric-btn success">Thêm vào gói dịch vụ của bạn</a>
         
            
        </div>
    </div>
</div>
